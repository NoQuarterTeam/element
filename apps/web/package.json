    "lint": "eslint --config .eslintrc.js --ext .ts,.tsx app",
    "prettier": "prettier --check app",
    "prettify": "prettier --write app",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
