{
  "name": "@element/app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "APP_VARIANT=development expo start --ios",
    "build:all": "pnpm run build:production && pnpm run build:development && pnpm run build:preview",
    "build:production": "eas build --profile production --platform all --auto-submit",
    "build:preview": "eas build --profile preview --platform all",
    "build:development": "eas build --profile development --platform all",
    "lint:fix": "eslint --config .eslintrc.commit.js --fix --cache --ext .ts,.tsx .",
    "lint": "eslint --config .eslintrc.js --ext .ts,.tsx .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "pnpm run lint:fix && pnpm run prettier:fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .expo .turbo node_modules"
  },
  "dependencies": {
    "@element/api": "workspace:*",
    "@element/database": "workspace:*",
    "@element/shared": "workspace:*",
    "@element/tailwind-config": "workspace:*",
    "@expo-google-fonts/poppins": "0.2.3",
    "@expo/react-native-action-sheet": "4.0.1",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-picker/picker": "2.4.10",
    "@tanstack/react-query": "4.23.0",
    "@trpc/client": "10.45.0",
    "@trpc/react-query": "10.45.0",
    "class-variance-authority": "0.7.0",
    "dayjs": "1.11.10",
    "expo": "~49.0.21",
    "expo-application": "~5.3.1",
    "expo-blur": "~12.4.1",
    "expo-constants": "~14.4.2",
    "expo-font": "~11.4.0",
    "expo-haptics": "~12.4.0",
    "expo-image": "~1.3.5",
    "expo-image-picker": "~14.3.2",
    "expo-linking": "~5.0.2",
    "expo-modules-autolinking": "~1.5.1",
    "expo-router": "2.0.14",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "2.4.0",
    "expo-updates": "~0.18.18",
    "lucide-react-native": "0.300.0",
    "nativewind": "2.0.11",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "2.12.1",
    "react-native-modal-datetime-picker": "14.0.1",
    "react-native-progress": "5.0.1",
    "react-native-reanimated": "3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.1",
    "react-native-svg": "13.9.0",
    "react-native-toast-message": "2.2.0",
    "reanimated-color-picker": "2.0.0",
    "superjson": "1.12.2",
    "uuid": "3.4.0",
    "zustand": "4.4.4"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@babel/preset-env": "7.23.7",
    "@babel/runtime": "7.23.7",
    "@element/eslint-config": "*",
    "@element/tailwind-config": "*",
    "@expo/config-plugins": "7.2.5",
    "@types/react": "18.2.31",
    "@types/react-native": "0.72.0",
    "@types/uuid": "3.4.13",
    "@typescript-eslint/eslint-plugin": "6.15.0",
