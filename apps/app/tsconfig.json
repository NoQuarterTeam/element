{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-native",
    "types": [
      "nativewind/types"
    ],
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "expo-plugins",
    "src",
    "app.config.ts",
    "index.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}