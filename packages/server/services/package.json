{
  "name": "@element/server-services",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": false,
  "scripts": {
    "lint:fix": "eslint --config .eslintrc.commit.js --fix --cache --ext .ts,.tsx src",
    "lint": "eslint --config .eslintrc.js --ext .ts,.tsx src",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "format": "pnpm run lint:fix && pnpm run prettier:fix",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../../.env --"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.478.0",
    "@aws-sdk/lib-storage": "3.478.0",
    "@aws-sdk/s3-request-presigner": "3.478.0",
    "@element/database": "workspace:*",
    "@element/emails": "workspace:*",
    "@element/server-env": "workspace:*",
    "@element/shared": "workspace:*",
    "@slack/web-api": "6.11.0",
    "bcryptjs": "2.4.3",
    "blurhash": "2.0.5",
    "dayjs": "1.11.10",
    "flickr-sdk": "7.0.0-beta.7",
    "stripe": "14.1.0",
    "jsonwebtoken": "9.0.2",
    "loops": "^0.1.3",
    "nodemailer": "6.9.7",
    "resend": "2.0.0",
    "sharp": "0.32.6",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@element/eslint-config": "workspace:*",
    "@types/bcryptjs": "2.4.6",
    "@types/jsonwebtoken": "9.0.5",
    "@types/node": "18.19.4",
    "@types/nodemailer": "6.4.14",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "dotenv-cli": "7.3.0",
    "eslint": "8.56.0",
    "prettier": "3.1.1",
    "typescript": "5.3.3"
  }
}
